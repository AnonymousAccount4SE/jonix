{"recordReference":{"value":"com.globalbookinfo.onix.01734529"},"notificationType":{"value":"Notification_confirmed_on_publication"},"productIdentifiers":[{"productIDType":{"value":"GTIN_13"},"idValue":{"value":"9780007232833"}},{"productIDType":{"value":"ISBN_13"},"idValue":{"value":"9780007232833"}}],"recordSourceType":{"value":"Bibliographic_agency"},"recordSourceName":{"value":"Global Bookinfo"},"descriptiveDetail":{"productComposition":{"value":"Single_component_retail_product"},"productForm":{"value":"Paperback_softback"},"titleDetails":[{"titleType":{"value":"Distinctive_title_book"},"titleElements":[{"titleElementLevel":{"value":"Product"},"titleWithoutPrefix":{"textcase":"Sentence_case","value":"Roseanna"},"sequenceNumber":{"value":1},"noPrefix":{}}]},{"titleType":{"value":"Distributor_s_title"},"titleElements":[{"titleElementLevel":{"value":"Product"},"titleText":{"value":"ROSEANNA (MARTIN BECK #1)"}}]}],"contributors":[{"keyNames":{"value":"Sjöwall"},"contributorRoles":[{"value":"By_author"}],"nameIdentifiers":[{"nameIDType":{"value":"Proprietary"},"idValue":{"value":"7421"},"idTypeName":{"value":"HCP Author ID"}},{"nameIDType":{"value":"ISNI"},"idValue":{"value":"0000000121479135"}}],"sequenceNumber":{"value":1},"namesBeforeKey":{"value":"Maj"},"biographicalNotes":[{"textformat":"XHTML","value":"<p><strong>Maj Sjöwall</strong> was born in Stockholm in 1935. She is a poet, novelist and translator, and is best known for the ten <em>Martin Beck</em> novels she wrote with husband Per Wahlöö.</p>"}]},{"keyNames":{"value":"Wahlöö"},"contributorRoles":[{"value":"By_author"}],"nameIdentifiers":[{"nameIDType":{"value":"Proprietary"},"idValue":{"value":"7422"},"idTypeName":{"value":"HCP Author ID"}},{"nameIDType":{"value":"ISNI"},"idValue":{"value":"0000000121222604"}}],"sequenceNumber":{"value":2},"namesBeforeKey":{"value":"Per"},"biographicalNotes":[{"textformat":"XHTML","value":"<p><strong>Per Wahlöö</strong> was born in Göteborg. After graduating from the University of Lund in 1946, he worked as a journalist, covering criminal and social issues for a number of newspapers and magazines. In the 1950s, Wahlöö became involved with radical political causes, activities that resulted in his deportation from Franco’s Spain in 1957. After returning to Sweden, he wrote a number of television and radio plays, and was managing editor of several magazines, before becoming a full-time writer.</p><p>He is best known for the series of ten <em>Martin Beck</em> novels he wrote with wife Maj Sjöwall, which they completed immediately before his death in 1975.</p>"}]},{"keyNames":{"value":"Roth"},"contributorRoles":[{"value":"Translated_by"}],"nameIdentifiers":[{"nameIDType":{"value":"Proprietary"},"idValue":{"value":"11150"},"idTypeName":{"value":"HCP Author ID"}}],"sequenceNumber":{"value":3},"namesBeforeKey":{"value":"Lois"},"fromLanguages":[{"value":"Swedish"}]},{"keyNames":{"value":"Mankell"},"contributorRoles":[{"value":"Introduction_by"}],"nameIdentifiers":[{"nameIDType":{"value":"Proprietary"},"idValue":{"value":"11151"},"idTypeName":{"value":"HCP Author ID"}},{"nameIDType":{"value":"ISNI"},"idValue":{"value":"0000000122824957"}}],"sequenceNumber":{"value":4},"namesBeforeKey":{"value":"Henning"}}],"countryOfManufacture":{"value":"United_Kingdom"},"noEdition":{},"productFormDetails":[{"value":"B_format_paperback"}],"measures":[{"measureType":{"value":"Height"},"measurement":{"value":197.0},"measureUnitCode":{"value":"Millimeters"}},{"measureType":{"value":"Width"},"measurement":{"value":130.0},"measureUnitCode":{"value":"Millimeters"}},{"measureType":{"value":"Thickness"},"measurement":{"value":18.0},"measureUnitCode":{"value":"Millimeters"}},{"measureType":{"value":"Unit_weight"},"measurement":{"value":195.0},"measureUnitCode":{"value":"Grams"}}],"productClassifications":[{"productClassificationType":{"value":"HMRC"},"productClassificationCode":{"value":"49019900"}}],"collections":[{"collectionType":{"value":"Publisher_collection"},"titleDetails":[{"titleType":{"value":"Distinctive_title_book"},"titleElements":[{"titleElementLevel":{"value":"Collection_level"},"titleWithoutPrefix":{"textcase":"Sentence_case","value":"Martin Beck series"},"sequenceNumber":{"value":2},"titlePrefix":{"textcase":"Sentence_case","value":"The"}},{"titleElementLevel":{"value":"Product"},"partNumber":{"value":"1"},"sequenceNumber":{"value":3}}]}]}],"contributorStatements":[{"value":"By Maj Sjöwall and Per Wahlöö, translated by Lois Roth"}],"languages":[{"languageRole":{"value":"Language_of_text"},"languageCode":{"value":"English"}},{"languageRole":{"value":"Original_language_of_a_translated_text"},"languageCode":{"value":"Swedish"}}],"extents":[{"extentType":{"value":"Main_content_page_count"},"extentValue":{"value":245.0},"extentUnit":{"value":"Pages"}},{"extentType":{"value":"Back_matter_page_count"},"extentValue":{"value":16.0},"extentUnit":{"value":"Pages"}}],"subjects":[{"subjectSchemeIdentifier":{"value":"Thema_subject_category"},"subjectCode":{"value":"FFP"},"mainSubject":{},"subjectSchemeVersion":{"value":"1.3"}},{"subjectSchemeIdentifier":{"value":"Thema_subject_category"},"subjectCode":{"value":"FYT"},"subjectSchemeVersion":{"value":"1.3"}},{"subjectSchemeIdentifier":{"value":"Thema_place_qualifier"},"subjectCode":{"value":"1DNS"},"subjectSchemeVersion":{"value":"1.3"}},{"subjectSchemeIdentifier":{"value":"Thema_time_period_qualifier"},"subjectCode":{"value":"3MPQS"},"subjectSchemeVersion":{"value":"1.3"}},{"subjectSchemeIdentifier":{"value":"BISAC_Subject_Heading"},"subjectCode":{"value":"FIC022000"},"mainSubject":{},"subjectSchemeVersion":{"value":"2009"}},{"subjectSchemeIdentifier":{"value":"Keywords"},"subjectHeadingTexts":[{"value":"Martin Beck; Roseanna McGraw; Lake Vättern; Stockholm; police procedural"}]}],"audiences":[{"audienceCodeType":{"value":"ONIX_audience_codes"},"audienceCodeValue":{"value":"01"}},{"audienceCodeType":{"value":"ONIX_Adult_audience_rating"},"audienceCodeValue":{"value":"01"}}]},"collateralDetail":{"textContents":[{"textType":{"value":"Short_description_annotation"},"contentAudiences":[{"value":"Unrestricted"}],"texts":[{"textformat":"XHTML","value":"<p><strong>Perennial relaunches the first novel in the classic Martin Beck detective series from the 1960s</strong> – the novels that have inspired all crime fiction written ever since.</p>"}]},{"textType":{"value":"Description"},"contentAudiences":[{"value":"Unrestricted"}],"texts":[{"textformat":"XHTML","value":"<p>Widely recognised as the among the greatest crime fiction ever written, this is the first of a series of stories that pioneered the police procedural genre. The series was translated into 35 languages, sold over 10 million copies around the world, and inspired writers from Henning Mankell to Jonathan Franzen.</p><p>Written in 1965, <em>Roseanna</em> is the work of Maj Sjöwall and Per Wahlöö – a husband and wife team from Sweden, and this volume has a new introduction to help bring their work to a new audience. The novel follows the fortunes of the detective Martin Beck, whose enigmatic and taciturn character has inspired countless other policemen in crime fiction.</p><p><em>Roseanna</em> begins on a July afternoon: the body of a young woman is dredged from a canal near Sweden’s beautiful Lake Vättern. Three months later, all that Police Inspector Martin Beck knows is that her name is Roseanna, that she came from Lincoln, Nebraska, and that she could have been strangled by any one of eighty-five people.</p><p>With its authentically rendered settings and vividly realized characters, and its command over the intricately woven details of police detection, <em>Roseanna</em> is a masterpiece of suspense and sadness.</p>"}]},{"textType":{"value":"Review_quote"},"contentAudiences":[{"value":"Unrestricted"}],"texts":[{"textformat":"XHTML","value":"<p>‘The writing is elegant and surprisingly humorous – if you haven’t come across Beck before, you’re in for a treat.’</p>"}],"sourceTitles":[{"value":"The Guardian"}]},{"textType":{"value":"Review_quote"},"contentAudiences":[{"value":"Unrestricted"}],"texts":[{"textformat":"XHTML","value":"<p>‘Their mysteries don’t just read well; they reread even better. Witness, wife, petty cop or crook – they’re all real characters even if they get just a few sentences. The plots hold, because they’re ingenious but never inhuman.’</p>"}],"sourceTitles":[{"value":"New York Times"}]},{"textType":{"value":"Promotional_headline"},"contentAudiences":[{"value":"Unrestricted"}],"texts":[{"value":"One of the greatest masterpieces of crime fiction"}]}],"citedContents":[{"citedContentType":{"value":"Media_mention"},"sourceTitles":[{"value":"Observer Magazine"}],"sourceType":{"value":"Printed_media"},"contentAudiences":[{"value":"Unrestricted"}],"citationNotes":[{"value":"Interview with Maj Sjöwall by Louise France"}],"resourceLinks":[{"value":"http://www.guardian.co.uk/books/2009/nov/22/crime-thriller-maj-sjowall-sweden"}],"contentDates":[{"contentDateRole":{"value":"Publication_date"},"date":{"dateformat":"YYYYMMDD","value":"20091122"}}]}],"supportingResources":[{"resourceContentType":{"value":"Front_cover"},"resourceMode":{"value":"Image"},"contentAudiences":[{"value":"Unrestricted"}],"resourceVersions":[{"resourceForm":{"value":"Downloadable_file"},"resourceLinks":[{"value":"http://www.harpercollins.co.uk/covers/9780007232833.jpg"}],"resourceVersionFeatures":[{"resourceVersionFeatureType":{"value":"File_format"},"featureValue":{"value":"D502"}},{"resourceVersionFeatureType":{"value":"Image_height_in_pixels"},"featureValue":{"value":"341"}},{"resourceVersionFeatureType":{"value":"Image_width_in_pixels"},"featureValue":{"value":"222"}}],"contentDates":[{"contentDateRole":{"value":"Last_updated"},"date":{"dateformat":"YYYYMMDD","value":"20060412"}}]}]},{"resourceContentType":{"value":"Sample_content"},"resourceMode":{"value":"Multi_mode"},"contentAudiences":[{"value":"Unrestricted"}],"resourceVersions":[{"resourceForm":{"value":"L